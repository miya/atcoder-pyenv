{
	"name": "atcoder-pyenv",
	"dockerComposeFile": [
		"../docker-compose.yml",
	],
	"service": "dev",
	"workspaceFolder": "/workspace",
	"settings": {
		"terminal.integrated.shell.linux": null
	},
	"extensions": [
		"ms-vscode-remote.remote-containers",
		"ms-pyright.pyright",
		"coenraads.bracket-pair-colorizer",
		"ms-python.python",
	],
	"shutdownAction": "stopCompose"
}
